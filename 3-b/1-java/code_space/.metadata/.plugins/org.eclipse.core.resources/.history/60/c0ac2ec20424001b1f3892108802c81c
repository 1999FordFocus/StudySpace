package kmp;

/**
 * KMP算法
 *
 */
public class Kmp {
	//第一步：构造next表
	public static int[] buildNext(String p) {
		int[] N = new int[p.length()];
		int m = p.length();
		int j = 0;
		int t = N[0]-1;
		while(j < m -1) {
			if(t < 0 || p.charAt(j) == p.charAt(t)) {
				j++;
				t++;
				N[j] = t;
			}else {
				t = N[t];
			}
		}
				
		return N;
	}
	
	public static void kmp() {
		
		
	}
}
